<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>fh_family_experience_web</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="fh-family-experience-api\**" />
    <Compile Remove="fh-family-experience-core\**" />
    <Compile Remove="fh-family-experience-infrastructure\**" />
    <Compile Remove="fh-family-experience-sharedkernel\**" />
    <Content Remove="fh-family-experience-api\**" />
    <Content Remove="fh-family-experience-core\**" />
    <Content Remove="fh-family-experience-infrastructure\**" />
    <Content Remove="fh-family-experience-sharedkernel\**" />
    <EmbeddedResource Remove="fh-family-experience-api\**" />
    <EmbeddedResource Remove="fh-family-experience-core\**" />
    <EmbeddedResource Remove="fh-family-experience-infrastructure\**" />
    <EmbeddedResource Remove="fh-family-experience-sharedkernel\**" />
    <None Remove="fh-family-experience-api\**" />
    <None Remove="fh-family-experience-core\**" />
    <None Remove="fh-family-experience-infrastructure\**" />
    <None Remove="fh-family-experience-sharedkernel\**" />
  </ItemGroup>

  <ItemGroup>
    <None Include="wwwroot\js\site.js" />
    <None Include="wwwroot\lib\jquery-validation-unobtrusive\jquery.validate.unobtrusive.js" />
    <None Include="wwwroot\lib\jquery-validation-unobtrusive\jquery.validate.unobtrusive.min.js" />
    <None Include="wwwroot\lib\jquery-validation\dist\additional-methods.js" />
